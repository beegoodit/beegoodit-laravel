{
    "name": "beegoodit/filament-partners",
    "description": "Partners and sponsors management for Filament with polymorphic scope and active period.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "partners",
        "sponsors",
        "polymorphic",
        "beegoodit"
    ],
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0 || ^5.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "spatie/eloquent-sortable": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\FilamentPartners\\": "src/",
            "BeegoodIT\\FilamentPartners\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\FilamentPartners\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\FilamentPartners\\FilamentPartnersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
