{
    "name": "beegoodit/laravel-cookie-consent",
    "description": "GDPR-compliant cookie consent banner for Laravel applications",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "cookie", "consent", "gdpr", "livewire", "beegoodit"],
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "livewire/livewire": "^3.0|^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\LaravelCookieConsent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\LaravelCookieConsent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\LaravelCookieConsent\\LaravelCookieConsentServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

