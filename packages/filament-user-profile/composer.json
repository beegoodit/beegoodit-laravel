{
    "name": "beegoodit/filament-user-profile",
    "description": "User profile settings pages for Filament applications",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "filament", "user", "profile", "settings", "beegoodit"],
    "authors": [
        {
            "name": "BeeGoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "filament/filament": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeeGoodIT\\FilamentUserProfile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeeGoodIT\\FilamentUserProfile\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeeGoodIT\\FilamentUserProfile\\FilamentUserProfileServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

