{
    "name": "beegoodit/laravel-pwa",
    "description": "Progressive Web App support for Laravel applications with push notifications",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "pwa",
        "progressive-web-app",
        "service-worker",
        "manifest",
        "push-notifications",
        "web-push",
        "vapid",
        "beegoodit"
    ],
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "minishlink/web-push": "^10.0",
        "spatie/laravel-settings": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\LaravelPwa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\LaravelPwa\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\LaravelPwa\\LaravelPwaServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}