{
    "name": "beegoodit/filament-legal",
    "description": "Centralized legal compliance for Filament applications",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "legal",
        "privacy",
        "gdpr",
        "beegoodit"
    ],
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "filament/filament": "^4.0 || ^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\FilamentLegal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\FilamentLegal\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\FilamentLegal\\FilamentLegalServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}