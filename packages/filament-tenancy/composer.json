{
    "name": "beegoodit/filament-tenancy",
    "description": "Multi-tenancy support for Filament applications with team branding and management",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "filament", "tenancy", "multi-tenant", "team", "branding", "logo", "colors", "beegoodit"],
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "filament/filament": "^4.0",
        "beegoodit/laravel-file-storage": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\FilamentTenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\FilamentTenancy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\FilamentTenancy\\FilamentTenancyServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

