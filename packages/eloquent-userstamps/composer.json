{
    "name": "beegoodit/eloquent-userstamps",
    "description": "Automatically track which user created and updated Eloquent models",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "eloquent", "userstamps", "audit", "beegoodit"],
    "authors": [
        {
            "name": "BeeGoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "BeeGoodIT\\EloquentUserstamps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeeGoodIT\\EloquentUserstamps\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "format": "pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

