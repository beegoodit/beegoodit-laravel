{
    "name": "beegoodit/filament-timeline",
    "description": "Abstract chronological history feed for Filament applications",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "timeline",
        "history",
        "events",
        "beegoodit"
    ],
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "livewire/livewire": "^3.0",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\FilamentTimeline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\FilamentTimeline\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\FilamentTimeline\\FilamentTimelineServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}