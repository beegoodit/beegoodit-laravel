{
    "name": "beegoodit/filament-tenancy-roles",
    "description": "Role-based authorization for Filament multi-tenancy.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "filament/filament": "^4.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0|^4.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\FilamentTenancyRoles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\FilamentTenancyRoles\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\FilamentTenancyRoles\\FilamentTenancyRolesServiceProvider"
            ]
        }
    }
}