{
    "name": "beegoodit/filament-i18n",
    "description": "User internationalization preferences (locale, timezone, time format) for Filament applications",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "i18n",
        "locale",
        "timezone",
        "beegoodit"
    ],
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "filament/filament": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\FilamentI18n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\FilamentI18n\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\FilamentI18n\\FilamentI18nServiceProvider"
            ],
            "aliases": {
                "FilamentI18n": "BeegoodIT\\FilamentI18n\\Facades\\FilamentI18n"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}