{
    "name": "beegoodit/filament-oauth",
    "description": "OAuth2 authentication with automatic team assignment for Filament applications",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "oauth",
        "microsoft",
        "teams",
        "beegoodit"
    ],
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "filament/filament": "^4.0",
        "dutchcodingcompany/filament-socialite": "^3.0",
        "laravel/socialite": "^5.23",
        "socialiteproviders/microsoft": "^4.7",
        "socialiteproviders/discord": "^4.2",
        "beegoodit/filament-user-avatar": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\FilamentOAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\FilamentOAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\FilamentOAuth\\FilamentOAuthServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint",
        "post-install-cmd": [
            "@php -r \"echo '\\n\\033[1;33mðŸ“¦ FilamentOAuth installed!\\033[0m\\n';\"",
            "@php -r \"echo '\\033[0;33mDon\\'t forget to publish migrations:\\033[0m\\n';\"",
            "@php -r \"echo '  php artisan vendor:publish --tag=filament-socialite-migrations\\n';\"",
            "@php -r \"echo '  php artisan vendor:publish --tag=filament-oauth-migrations\\n';\"",
            "@php -r \"echo '  php artisan migrate\\n\\n';\""
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}