{
    "name": "beegoodit/filament-tenancy-domains",
    "description": "Polymorphic domain and subdomain management for Filament applications",
    "license": "MIT",
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "contact@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "filament/filament": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\FilamentTenancyDomains\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\FilamentTenancyDomains\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\FilamentTenancyDomains\\FilamentTenancyDomainsServiceProvider"
            ]
        }
    }
}