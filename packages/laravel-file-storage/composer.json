{
    "name": "beegoodit/laravel-file-storage",
    "description": "Unified file storage with automatic S3/local disk selection and URL generation",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "storage", "s3", "files", "beegoodit"],
    "authors": [
        {
            "name": "BeegoodIT",
            "email": "dev@beegoodit.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\LaravelFileStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeegoodIT\\LaravelFileStorage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeegoodIT\\LaravelFileStorage\\LaravelFileStorageServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

