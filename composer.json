{
    "name": "beegoodit/laravel-packages",
    "description": "BeegoodIT Laravel package monorepo",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.1",
        "filament/filament": "^5.2",
        "laravel/pint": "^1.0",
        "livewire/livewire": "^4.1",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0",
        "rector/rector": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "BeegoodIT\\EloquentUserstamps\\": "packages/eloquent-userstamps/src/",
            "BeegoodIT\\LaravelFileStorage\\": "packages/laravel-file-storage/src/",
            "BeegoodIT\\LaravelCookieConsent\\": "packages/laravel-cookie-consent/src/",
            "BeegoodIT\\LaravelFeedback\\": "packages/laravel-feedback/src/",
            "BeegoodIT\\FilamentI18n\\": "packages/filament-i18n/src/",
            "BeegoodIT\\FilamentUserAvatar\\": "packages/filament-user-avatar/src/",
            "BeegoodIT\\FilamentOAuth\\": "packages/filament-oauth/src/",
            "BeegoodIT\\FilamentTenancy\\": "packages/filament-tenancy/src/",
            "BeegoodIT\\FilamentUserProfile\\": "packages/filament-user-profile/src/",
            "BeegoodIT\\FilamentKnowledgeBase\\": "packages/filament-knowledge-base/src/",
            "BeegoodIT\\LaravelPwa\\": "packages/laravel-pwa/src/",
            "BeegoodIT\\FilamentSocialLinks\\": "packages/filament-social-links/src/",
            "BeegoodIT\\FilamentSocialLinks\\Database\\Seeders\\": "packages/filament-social-links/database/seeders/",
            "BeegoodIT\\FilamentSocialLinks\\Database\\Factories\\": "packages/filament-social-links/database/factories/",
            "BeegoodIT\\FilamentTenancyDomains\\": "packages/filament-tenancy-domains/src/",
            "BeegoodIT\\FilamentLegal\\": "packages/filament-legal/src/",
            "BeegoodIT\\FilamentTimeline\\": "packages/filament-timeline/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "BeegoodIT\\EloquentUserstamps\\Tests\\": "packages/eloquent-userstamps/tests/",
            "BeegoodIT\\LaravelFileStorage\\Tests\\": "packages/laravel-file-storage/tests/",
            "BeegoodIT\\LaravelCookieConsent\\Tests\\": "packages/laravel-cookie-consent/tests/",
            "BeegoodIT\\LaravelFeedback\\Tests\\": "packages/laravel-feedback/tests/",
            "BeegoodIT\\FilamentI18n\\Tests\\": "packages/filament-i18n/tests/",
            "BeegoodIT\\FilamentUserAvatar\\Tests\\": "packages/filament-user-avatar/tests/",
            "BeegoodIT\\FilamentOAuth\\Tests\\": "packages/filament-oauth/tests/",
            "BeegoodIT\\FilamentTenancy\\Tests\\": "packages/filament-tenancy/tests/",
            "BeegoodIT\\FilamentUserProfile\\Tests\\": "packages/filament-user-profile/tests/",
            "BeegoodIT\\LaravelPwa\\Tests\\": "packages/laravel-pwa/tests/",
            "BeegoodIT\\FilamentTenancyDomains\\Tests\\": "packages/filament-tenancy-domains/tests/",
            "BeegoodIT\\FilamentSocialLinks\\Tests\\": "packages/filament-social-links/tests/",
            "BeegoodIT\\FilamentLegal\\Tests\\": "packages/filament-legal/tests/",
            "BeegoodIT\\FilamentTimeline\\Tests\\": "packages/filament-timeline/tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text",
        "format": "pint",
        "rector": "vendor/bin/rector process --dry-run",
        "rector:fix": "vendor/bin/rector process"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
